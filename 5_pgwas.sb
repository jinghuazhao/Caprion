module purge
module load rhel7/default-ccl

function bgen()
{
  export chr=chr${SLURM_ARRAY_TASK_ID}
  plink2 --bgen ${interval}/${chr}.bgen ref-unknown --sample ${interval}/interval.sample --keep ${analysis}/work/caprion${suffix}.id2 \
         --export bgen-1.2 bits=8 --dosage-erase-threshold 0.001 \
         --set-missing-var-ids @:#_\$r_\$a --new-id-max-allele-len 680 \
         --out ${analysis}/work/${chr}
  bgenix -g ${analysis}/work/${chr}.bgen -index -clobber
}

function fastLR()
# fastGWA without --grm-sparse
{
  export phenocol=${SLURM_ARRAY_TASK_ID}
  export phenoname=$(awk 'NR==ENVIRON["phenocol"]{print $1}' ${analysis}/work/caprion${suffix}.varlist)
  export batch=${1}
  gcta-1.9 --mbgen ${analysis}/work/caprion.bgenlist \
           --sample ${analysis}/work/caprion.sample \
           --keep ${analysis}/work/caprion-${batch}${suffix}.id \
           --fastGWA-lr \
           --pheno ${analysis}/work/caprion-${batch}${suffix}.pheno --mpheno ${phenocol} \
           --threads 10 \
           --out ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}

  gcta-1.9 --mbgen ${analysis}/work/caprion.bgenlist \
           --sample ${analysis}/work/caprion.sample \
           --keep ${analysis}/work/chrX-${batch}${suffix}.id \
           --fastGWA-lr --model-only \
           --pheno ${analysis}/work/caprion-${batch}${suffix}.pheno --mpheno ${phenocol} \
           --threads 10 \
           --out ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}

  gcta-1.9 --bgen ${analysis}/work/chrX.bgen \
           --sample ${analysis}/work/chrX.sample \
           --keep ${analysis}/work/chrX-${batch}${suffix}.id \
           --load-model ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}.fastGWA \
           --extract ${analysis}/work/chrX.snplist --geno 0.1 \
           --threads 10 \
           --out ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}-chrX
  bgzip -f ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}.fastGWA
  bgzip -f ${analysis}/pgwas${suffix}/caprion-${batch}-${phenoname}${suffix}-chrX.fastGWA
}

